%td= category.display_name
%td= category.category_type_humanize
%td= boolean_icon(category.enabled)
%td.text-right
  %ul.button-group.radius
    %li= react_component('GetButton', path: edit_category_path(category), icon: 'pencil')
    %li= react_component('DeleteButton', path: category_path(category), confirm: "Delete Category #{category.name}")
