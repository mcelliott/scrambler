.small-12.columns
  - if round.teams.present?
    .small-12.columns
      %ul
        - round.teams.each do |team|
          %li{ class: "team_#{team.id}" }
            = render partial: 'teams/team', locals: { team: team }
  - else
    .panel
      .empty-list.text-center= fa_icon('frown-o', text: "No Teams for Round #{round.round_number}")
